{
  "compilerOptions": {
    "types": ["@element-plus/icons-vue", "@vueuse/shared", "vite/client"],
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "checkJs": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true,
    "paths": {
      "@/*": ["./src/*"],
      "@api": ["./src/api"],
      "@utils/*": ["./src/utils/*"]
    }
  },
  //"exclude": ["node_modules", "dist"],
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.js", "src/**/*.vue"]
}
